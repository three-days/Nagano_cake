<h1>会員一覧画面</h1>
     <table class="table">
        <thead>
          <tr>
            <th>顧客ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
          </tr>
        </thead>
        <tbody>
            <% @admins_users.each do |p| %>
             <tr>
              <td><%= p.id %></td>
              <td><%= link_to p.family_name_kanji, admins_user_path(p.id)  %><%= link_to p.first_name_kanji, admins_user_path(p.id)  %></td>
              <td><%= p.email  %></td>
              <td><%= p.leave_status  %></td>
             </tr>
            <% end %>
        </tbody>
      </table>
    </div>
